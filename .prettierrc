{
  "semi": false,
  "bracketSpacing": true,
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "requirePragma": false,
  "htmlWhitespaceSensitivity": "strict",
  "singleAttributePerLine": true
}
