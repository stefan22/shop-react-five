{
  "semi": false,
  "bracketSpacing": true,
  "singleQuote": true,
  "printWidth": 90,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "requirePragma": true,
  "no-restricted-syntax": ["error", "SequenceExpression"]
}
